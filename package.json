{
	"name": "@douganderson444/svelte-fillvert",
	"version": "2.0.0",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-package",
		"publish:npm": "npm publish ./package --access public",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"import": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^4.2.7"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.3.1",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-preprocess": "^5.1.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/DougAnderson444/svelte-fillvert.git"
	},
	"keywords": [
		"svelte",
		"fillvert",
		"fill",
		"vertical",
		"flex",
		"flexbox"
	],
	"author": "Doug Anderson",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/DougAnderson444/svelte-fillvert"
	},
	"homepage": "https://github.com/DougAnderson444/svelte-fillvert"
}
